                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="-1"/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
                <Image x:Name="Imprimer" Height="50" Width="50" Source="../../Images/printer67.png" OpacityMask="#FFACAC38">
                    <Image.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform Angle="0.156"/>
                            <TranslateTransform Y="0.004"/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
            </Button>

            <cefSharp:ChromiumWebBrowser x:Name="CefHistoCollabo" Margin = "394,10,395,256"/>
            <!--Image x:Name = "graph_vitesse" Source = "D:/VeloPublic/VeloPublic/Images/Graph_1.png" OpacityMask = "#FFACAC38" Margin = "394,10,395,256" /-->

